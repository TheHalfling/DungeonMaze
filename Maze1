def main_run(current, roomData):
    print ("Welcome to the Dungeon Maze #1.  Can you find your way out??")
    print ("You are in room {}. There are exits to the ".format(currentRoom))
    roomCount = (roomData[currentRoom[directions]])
    print ("There are {} exits from the room. ".format(roomCount))


main_run(currentRoom, roomData)
